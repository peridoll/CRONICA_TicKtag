{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      {
        "when": "mode_twilight",
        "model": {
          "type": "minecraft:condition",
          "on_false": {
            "type": "minecraft:model",
            "model": "cronica:item/character/ikaros/weapon/apollon_bow/mode_twilight_default"
          },
          "on_true": {
            "type": "minecraft:range_dispatch",
            "entries": [
              {
                "model": {
                  "type": "minecraft:model",
                  "model": "cronica:item/character/ikaros/weapon/apollon_bow/mode_twilight_pulling_1"
                },
                "threshold": 0.65
              },
              {
                "model": {
                  "type": "minecraft:model",
                  "model": "cronica:item/character/ikaros/weapon/apollon_bow/mode_twilight_pulling_2"
                },
                "threshold": 0.9
              }
            ],
            "fallback": {
              "type": "minecraft:model",
              "model": "cronica:item/character/ikaros/weapon/apollon_bow/mode_twilight_pulling_0"
            },
            "property": "minecraft:use_duration",
            "scale": 0.05
          },
          "property": "minecraft:using_item"
        }
      },
      {
